  {
   &quot;cell_type&quot;: &quot;code&quot;,
   &quot;execution_count&quot;: null,
   &quot;metadata&quot;: {},
   &quot;outputs&quot;: [],
   &quot;source&quot;: [
    &quot;from pyspark.sql.functions import udf\n&quot;,
    &quot;@udf(\&quot;double\&quot;)\n&quot;,
    &quot;def normalize(s):\n&quot;,
    &quot;  return (s - min_ash) / (max_ash - min_ash)&quot;
   ]
