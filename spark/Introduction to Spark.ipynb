   &quot;cell_type&quot;: &quot;code&quot;,
   &quot;execution_count&quot;: null,
   &quot;metadata&quot;: {},
   &quot;outputs&quot;: [],
   &quot;source&quot;: [
    &quot;normed = df.select(\&quot;ash\&quot;, normalize(\&quot;ash\&quot;).alias(\&quot;ash_normed\&quot;))\n&quot;,
